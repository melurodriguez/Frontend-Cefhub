{
  "expo": {
    "name": "ChefHub",
    "slug": "ChefHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoChefHub.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "schema": "chefhub",
    "splash": {
      "image": "./assets/iconn.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoChefHub.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ChefHub"
    },
    "web": {
      "favicon": "./assets/LogoChefHub.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-font",
      "expo-video",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f2517d28-c1b5-4870-824a-106c49b06c6a"
      }
    },
    "owner": "melurodriguez"
  }
}
